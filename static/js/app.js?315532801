  if($('.post-video-player-wrapper').find('.post-list-thumbnails').length > 0){
    var wrapper = $('.post-video-player-wrapper');
    setTimeout(function(){
      var height = $(wrapper).find('.post-cover').outerHeight() - 57;
      $(wrapper).find('.post-list-thumbnails').css('height', height + 'px');
      $(wrapper).find('.post-video-player-playlist').removeClass('hidden');
