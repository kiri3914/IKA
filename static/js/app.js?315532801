          maxlength: i18next.t("Максимальная длина пароля 255 символов")
        }
      }
    });
  });

  $('.profile-change-post-status-form').each(function(){
    $(this).validate({
      rules: {
        status: {
          required: true
        },
        score: {
          required: true,
          number: true,
          min: parseInt($(this).find('input[name=score]').attr('min')),
          max: 100
        },
        reason: {
          required: true,
          minlength: 2,
          maxlength: 500
        }
      },
      messages: {
        status: {
          required: i18next.t("Поле обязательно для заполнения")
        },
        score: {
          required: i18next.t("Поле обязательно для заполнения"),
          number: i18next.t("Введите корректное число"),
          min: i18next.t("Значение должно быть больше или равно {0}", {0: parseInt($(this).find('input[name=score]').attr('min'))}),
          max: i18next.t("Значение должно быть меньше или равно 100")
        },
        reason: {
          required: i18next.t("Поле обязательно для заполнения"),
          minlength: i18next.t("Минимальная длина строки 2 символов"),
          maxlength: i18next.t("Максимальная длина пароля 500 символов")
        }
      },
      submitHandler: function(form) {
        $(form).find('.btn-submit').addClass('disabled').attr('disabled', true).blur();
        form.submit();
      }
    });
  });

  $('.profile-task-result-response-form').each(function(){
    $(this).validate({
      rules: {
        score: {
          required: true,
          number: true,
          min: 0,
          max: 100
        },
        comment: {
          required: true,
          minlength: 2,
          maxlength: 5000
        }
      },
      messages: {
        score: {
          required: i18next.t("Поле обязательно для заполнения"),
          number: i18next.t("Введите корректное число"),
          min: i18next.t("Значение должно быть больше или равно 0"),
          max: i18next.t("Значение должно быть меньше или равно 100")
        },
        comment: {
          required: i18next.t("Поле обязательно для заполнения"),
          minlength: i18next.t("Минимальная длина строки 2 символа"),
          maxlength: i18next.t("Максимальная длина строки 5000 символов")
        }
      },
      submitHandler: function(form) {
        $(form).find('.btn-submit').addClass('disabled').attr('disabled', true).blur();
        form.submit();
      }
    });
  });

});
$(document).ready(function() {

  function getMorePostComments(post_id, page){
    post_id = parseInt(post_id);
    page = parseInt(page) + 1;

    $.ajax({
      type: 'get',
      url: '/comments/ajax-get-comments-for-post/' + post_id + '/' + page,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if(response.page){
          $('#more-post-comments-toggle').removeClass('disabled').attr({
            'data-post-id': response.post_id,
            'data-page': response.page
          });
          $('#more-post-comments-toggle').find('i').attr('class', 'far fa-arrow-down');
        } else {
          $('#more-post-comments-toggle').remove();
        }
        if (response.content) {
          $('#comments-wrapper').append(response.content);
        }
      }
     /* error: function(e) {
        console.log(e);
      }*/
    });
  }

  function getMoreCourseComments(course_id, page){
    course_id = parseInt(course_id);
    page = parseInt(page) + 1;

    $.ajax({
      type: 'get',
      url: '/comments/ajax-get-comments-for-course/' + course_id + '/' + page,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if(response.page){
          $('#more-course-comments-toggle').removeClass('disabled').attr({
            'data-post-id': response.course_id,
            'data-page': response.page
          });
          $('#more-course-comments-toggle').find('i').attr('class', 'far fa-arrow-down');
        } else {
          $('#more-course-comments-toggle').remove();
        }
        if (response.content) {
          $('#comments-wrapper').append(response.content);
        }
      }
     /* error: function(e) {
        console.log(e);
      }*/
    });
  }

  function commentFormForPost(post_id, parent_comment_id){
    post_id = parseInt(post_id);
    parent_comment_id = parseInt(parent_comment_id);

    $.ajax({
      type: 'get',
      url: '/comments/ajax-comment-form-for-post/' + post_id + '/' + parent_comment_id,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if (response.content) {
          $('#comment-' + parent_comment_id).find('.comment-reply-form').html(response.content);
        }
      }
      /* error: function(e) {
       console.log(e);
       }*/
    });
  }

  function commentFormForCourse(course_id, parent_comment_id){
    course_id = parseInt(course_id);
    parent_comment_id = parseInt(parent_comment_id);

    $.ajax({
      type: 'get',
      url: '/comments/ajax-comment-form-for-course/' + course_id + '/' + parent_comment_id,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if (response.content) {
          $('#comment-' + parent_comment_id).find('.comment-reply-form').html(response.content);
        }
      }
      /* error: function(e) {
       console.log(e);
       }*/
    });
  }

  function commentEditForm(comment_id){
    comment_id = parseInt(comment_id);

    $.ajax({
      type: 'get',
      url: '/comments/ajax-comment-edit-form/' + comment_id,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if (response.content) {
          $('#comment-' + comment_id).find('.comment-edit-form').html(response.content);
        }
      }
      /* error: function(e) {
       console.log(e);
       }*/
    });
  }

  function updateCommentRating(comment_id){
    comment_id = parseInt(comment_id);

    $.ajax({
      type: 'post',
      url: '/comments/ajax-update-comment-rating/' + comment_id,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        $('#comment-like-btn-' + comment_id).removeClass('disabled');
        if (response.content) {
          $('#comment-likes-num-' + comment_id).find('span').html(response.content);
          if(parseInt(response.content) <= 0){
            $('#comment-likes-num-' + comment_id).addClass('hidden');
          } else {
            $('#comment-likes-num-' + comment_id).removeClass('hidden');
          }
        }
      },
      error: function(e) {
        //console.log(e);
      }
    });
  }

  $('#more-post-comments-toggle').on('click', function(){
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    $(this).find('i').attr('class', 'far fa-sync fa-spin');
    getMorePostComments($(this).attr('data-post-id'), $(this).attr('data-page'));
  });

  $('#more-course-comments-toggle').on('click', function(){
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    $(this).find('i').attr('class', 'far fa-sync fa-spin');
    getMoreCourseComments($(this).attr('data-course-id'), $(this).attr('data-page'));
  });

  $('#comments-wrapper').on('click', '.comment-reply-post-btn', function(e){
    e.preventDefault();
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    commentFormForPost($(this).attr('data-post-id'), $(this).attr('data-comment-id'));
  });

  $('#comments-wrapper').on('click', '.comment-reply-course-btn', function(e){
    e.preventDefault();
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    commentFormForCourse($(this).attr('data-course-id'), $(this).attr('data-comment-id'));
  });

  $('#comments-wrapper').on('click', '.comment-edit-btn', function(e){
    e.preventDefault();
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    commentEditForm($(this).attr('data-comment-id'));
  });

  $('#comments-wrapper').on('click', '.comment-cancel-reply-btn', function(e){
    e.preventDefault();
    var parent_comment_id = $(this).attr('data-comment-id');
    $('#comment-reply-form-' + parent_comment_id).remove();
    $('#comment-reply-btn-' + parent_comment_id).removeClass('disabled');
  });

  $('#comments-wrapper').on('click', '.comment-cancel-edit-btn', function(e){
    e.preventDefault();
    var parent_comment_id = $(this).attr('data-comment-id');
    $('#comment-edit-form-' + parent_comment_id).remove();
    $('#comment-edit-btn-' + parent_comment_id).removeClass('disabled');
  });

  $('#comments-wrapper').on('click', '.comment-like-btn', function(e){
    e.preventDefault();
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    updateCommentRating($(this).attr('data-comment-id'));
  });

});

$(document).ready(function() {

  function getMoreCourseReviews(course_id, page){
    course_id = parseInt(course_id);
    page = parseInt(page) + 1;

    $.ajax({
      type: 'get',
      url: '/reviews/ajax-get-reviews-for-course/' + course_id + '/' + page,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if(response.page){
          $('#more-course-reviews-toggle').removeClass('disabled').attr({
            'data-post-id': response.course_id,
            'data-page': response.page
          });
          $('#more-course-reviews-toggle').find('i').attr('class', 'far fa-arrow-down');
        } else {
          $('#more-course-reviews-toggle').remove();
        }
        if (response.content) {
          $('#course-reviews-wrapper').append(response.content);
        }
      }
      /*error: function(e) {
        console.log(e.responseText);
      }*/
    });
  }

  $('#more-course-reviews-toggle').on('click', function(){
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    $(this).find('i').attr('class', 'far fa-sync fa-spin');
    getMoreCourseReviews($(this).attr('data-course-id'), $(this).attr('data-page'));
  });

});
$(document).ready(function() {

  function taskResultEditForm(task_result_id){
    task_result_id = parseInt(task_result_id);

    $.ajax({
      type: 'get',
      url: '/task-results/ajax-task-result-edit-form/' + task_result_id,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if (response.content) {
          $('#task-result-' + task_result_id).find('.task-result-inner > .task-result-body').addClass('hidden');
          $('#task-result-' + task_result_id).find('.task-result-edit-form').html(response.content);
          customFileInputHandler();
        }
      }
      /* error: function(e) {
       console.log(e);
       }*/
    });
  }

  function customFileInputHandler(){
    // We can attach the `fileselect` event to all file inputs on the page
    $('.custom-file').on('change', ':file', function() {
      var input = $(this),
        numFiles = input.get(0).files ? input.get(0).files.length : 1,
        label = input.val().replace(/\\/g, '/').replace(/.*\//, '');
      input.trigger('fileselect', [numFiles, label]);
    });

    $(':file').on('fileselect', function(event, numFiles, label) {

      var input = $(this).parents('.custom-file').find(':text'),
        log = numFiles > 0 ? i18next.t("Выбрано файлов") + ': ' + numFiles : label;

      if( input.length ) {
        input.val(log);
      }
    });
  }


  $('#task-results-wrapper').on('click', '.task-result-edit-btn', function(e){
    e.preventDefault();
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    taskResultEditForm($(this).attr('data-task-result-id'));
  });

  $('#task-results-wrapper').on('click', '.task-result-cancel-edit-btn', function(e){
    e.preventDefault();
    var parent_task_result_id = $(this).attr('data-task-result-id');
    $('#task-result-edit-form-' + parent_task_result_id).remove();
    $('#task-result-' + parent_task_result_id).find('.task-result-inner > .task-result-body').removeClass('hidden');
    $('#task-result-edit-btn-' + parent_task_result_id).removeClass('disabled');
  });

  $('#task-results-wrapper').on('click', '.post-attachment-remove', function(){
    $(this).parents('.post-attachment-wrapper').remove();
  });
});
$(document).ready(function() {

  function markNotificationAsRead(notification_id){
    notification_id = parseInt(notification_id);

    $.ajax({
      type: 'post',
      url: '/notifications/ajax-mark-as-read/' + notification_id,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        //console.log(response.content);
      },
      error: function(e) {
        //console.log(e);
      }
    });
  }


  $('.navbar-notifications-list').on('click', '.navbar-notifications-list-item', function(e){
    //e.preventDefault();
    markNotificationAsRead($(this).attr('data-notification-id'));
  });

  $('.profile-body').on('click', '.navbar-notifications-list-item', function(e){
    //e.preventDefault();
    markNotificationAsRead($(this).attr('data-notification-id'));
  });
});

$(document).ready(function() {

  var quiz_form = $("#form-quiz");


  function generateValidationRules(){
    var validation_rules = {};

    $("#form-quiz").find("input:checkbox").each(function () {
      var input_name = $(this).attr('name');
      var input_group_id = $(this).attr('data-group-id');
      validation_rules[input_name] = {
        require_from_group: [1, ".option-group-" + input_group_id]
      };
    });

    $("#form-quiz").find("textarea").each(function () {
      var input_name = $(this).attr('name');
      var input_id = $(this).attr('data-option-id');
      validation_rules[input_name] = {
        //required: ".option-custom-" + input_id + ":checked",
        minlength: 2,
        maxlength: 1000
      };
    });

    $("#form-quiz").find(".post-star-rating").each(function () {
      var input_name = $(this).attr('data-option-input-name');
      var input_id = $(this).attr('data-option-id');
      validation_rules[input_name] = {
        required: ".option-custom-" + input_id + ":checked"
      };
    });

    return validation_rules;
  }

  quiz_form.validate({
    rules: generateValidationRules(),
    errorPlacement: function(error, element) {
      // Don't show error
    }
  });

  quiz_form.steps({
    headerTag: "h4",
    bodyTag: "fieldset",
    stepsOrientation: "horizontal",
    titleTemplate: '<span class="step">#index#</span>',
    cssClass: 'wizard wizard-simple',
    labels: {
      cancel: "Отмена",
      current: "Шаг:",
      pagination: "Pagination",
      finish: "Завершить",
      next: "<span>Далее</span><i class='far fa-arrow-right'></i>",
      previous: "<i class='far fa-arrow-left'></i>",
      loading: "Подождите..."
    },
    onInit: function (event, currentIndex) {
      quiz_form.removeClass('wizard-uninitialized');
    },
    onStepChanging: function (event, currentIndex, newIndex) {
      // Always allow previous action even if the current form is not valid!
      if (currentIndex > newIndex)
      {
        return true;
      }
      // Needed in some cases if the user went back (clean up)
      if (currentIndex < newIndex)
      {
        // To remove error styles
        quiz_form.find(".body:eq(" + newIndex + ") label.error").remove();
        quiz_form.find(".body:eq(" + newIndex + ") .error").removeClass("error");
      }

      return quiz_form.valid();
    },
    onStepChanged: function (event, currentIndex, priorIndex) {
      if (priorIndex > currentIndex)
      {
        quiz_form.find(".steps>ul>li:gt(" + currentIndex + ").done").removeClass("done").addClass("disabled");
        quiz_form.find(".steps>ul>li:eq(" + currentIndex + ").done").removeClass("done");
      }
    },
    onFinishing: function (event, currentIndex) {
      return quiz_form.valid();
    },
    onFinished: function (event, currentIndex) {
      quiz_form.submit();
    }
  });


  $(quiz_form).find(".post-quiz-item-type-radio").each(function () {
    var group = $(this);

    $(group).find("input:checkbox.checkbox-outline").each(function () {
      $(this).removeClass("checkbox-outline").addClass("radio-outline");
      $(this).change(function () {
        $(group).find("input:checkbox").not($(this)).prop("checked", false);
        $(this).prop("checked", true);

        $(group).find("input:checkbox.option-custom").not($(this)).each(function(){
          var option_id = $(this).val();
          $('.option-custom-' + option_id).collapse('hide');
          $('.option-custom-' + option_id + ' textarea').val('');
        });

        $('.option-custom-' + $(this).val()).collapse('show');
      });
    });

    $(group).find("input:checkbox.checkbox-image-lg").each(function () {
      $(this).removeClass("checkbox-image-lg").addClass("radio-image-lg");
      $(this).change(function () {
        $(group).find("input:checkbox").not($(this)).prop("checked", false);
        $(this).prop("checked", true);
      });
    });

    $(group).find("input:checkbox.checkbox-image-sm").each(function () {
      $(this).removeClass("checkbox-image-sm").addClass("radio-image-sm");
      $(this).change(function () {
        $(group).find("input:checkbox").not($(this)).prop("checked", false);
        $(this).prop("checked", true);
      });
    });
  });

  $(quiz_form).find('.post-quiz-item-type-checkbox').each(function () {
    var group = $(this);

    $(group).find("input:checkbox.option-custom").each(function () {
      $(this).change(function () {
        var option_id = $(this).val();
        if ($(this).is(":checked")) {
          $('.option-custom-' + option_id).collapse('show');
        } else {
          $('.option-custom-' + option_id).collapse('hide');
          $('.option-custom-' + option_id + ' textarea').val('');
        }
      });
    });
  });
});

$(document).ready(function() {
  function getSpecialOffer(order_id, product_id){
    order_id = parseInt(order_id);
    product_id = parseInt(product_id);

    $.ajax({
      type: 'get',
      url: '/orders/ajax-get-special-offer/' + order_id + '/' + product_id,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if (response.content) {
          $('#thank-you-special-offer-inner').html(response.content);
        } else {
          window.location.href = '/';
        }
      }
      /*error: function(e) {
        console.log(e.responseText);
      }*/
    });
  }

  $('#thank-you-special-offer').on('click', '#thank-you-special-offer-next', function(){
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    getSpecialOffer($(this).attr('data-order-id'), $(this).attr('data-product-id'));
  });
});
$(document).ready(function() {

  $('.club-onboarding-form-step-1').each(function () {
    $(this).validate({
      rules: {
        name: {
          required: true,
          minlength: 2,
          maxlength: 255
        },
        phone: {
          required: true,
          phone: true
        },
        description: {
          required: true,
          minlength: 2,
          maxlength: 255
        },
        bio: {
          minlength: 2,
          maxlength: 2000
        },
        occupation: {
          minlength: 2,
          maxlength: 255
        },
        expertise: {
          minlength: 2,
          maxlength: 255
        },
        city: {
          minlength: 2,
          maxlength: 255
        },
        site: {
          url: true,
          maxlength: 255
        },
      },
      messages: {
        name: {
          required: i18next.t("Поле обязательно для заполнения"),
          minlength: i18next.t("Минимальная длина строки 2 символа"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        phone: {
          required: i18next.t("Поле обязательно для заполнения"),
          phone: i18next.t("Введите корректный номер телефона")
        },
        description: {
          required: i18next.t("Поле обязательно для заполнения"),
          minlength: i18next.t("Минимальная длина строки 2 символа"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        bio: {
          minlength: i18next.t("Минимальная длина строки 2 символа"),
          maxlength: i18next.t("Максимальная длина строки 2000 символов")
        },
        occupation: {
          minlength: i18next.t("Минимальная длина строки 2 символа"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        expertise: {
          minlength: i18next.t("Минимальная длина строки 2 символа"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        city: {
          minlength: i18next.t("Минимальная длина строки 2 символа"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        site: {
          url: i18next.t("Введите корректную ссылку"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
      }
    });
  });

  $('.club-onboarding-form-step-3').each(function () {
    $(this).validate({
      rules: {
        telegram: {
          url: true,
          maxlength: 255
        },
        facebook: {
          url: true,
          maxlength: 255
        },
        linkedin: {
          url: true,
          maxlength: 255
        },
        instagram: {
          url: true,
          maxlength: 255
        },
        youtube: {
          url: true,
          maxlength: 255
        },
      },
      messages: {
        telegram: {
          url: i18next.t("Введите корректную ссылку"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        facebook: {
          url: i18next.t("Введите корректную ссылку"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        linkedin: {
          url: i18next.t("Введите корректную ссылку"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        instagram: {
          url: i18next.t("Введите корректную ссылку"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
        youtube: {
          url: i18next.t("Введите корректную ссылку"),
          maxlength: i18next.t("Максимальная длина строки 255 символов")
        },
      }
    });
  });

  function getMoreClubEventMembers(event_id, page){
    event_id = parseInt(event_id);
    page = parseInt(page) + 1;

    $.ajax({
      type: 'get',
      url: '/club-events/ajax-get-members-for-club-event/' + event_id + '/' + page,
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        if(response.page){
          $('#more-club-event-members-toggle').removeClass('disabled').attr({
            'data-event-id': response.event_id,
            'data-page': response.page
          });
          $('#more-club-event-members-toggle').find('i').attr('class', 'far fa-arrow-down');
        } else {
          $('#more-club-event-members-toggle').remove();
        }
        if (response.content) {
          $('#club-event-members-wrapper').append(response.content);
        }
      }
       /*error: function(e) {
         console.log(e);
       }*/
    });
  }

  $('#more-club-event-members-toggle').on('click', function(){
    if($(this).hasClass('disabled')) { return false; }
    $(this).addClass('disabled').blur();
    $(this).find('i').attr('class', 'far fa-sync fa-spin');
    getMoreClubEventMembers($(this).attr('data-event-id'), $(this).attr('data-page'));
  });
});
