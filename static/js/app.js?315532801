  function customFileInputHandler(){
    // We can attach the `fileselect` event to all file inputs on the page
    $('.custom-file').on('change', ':file', function() {
      var input = $(this),
        numFiles = input.get(0).files ? input.get(0).files.length : 1,
        label = input.val().replace(/\\/g, '/').replace(/.*\//, '');
      input.trigger('fileselect', [numFiles, label]);
    });

    $(':file').on('fileselect', function(event, numFiles, label) {

      var input = $(this).parents('.custom-file').find(':text'),
        log = numFiles > 0 ? i18next.t("Выбрано файлов") + ': ' + numFiles : label;

      if( input.length ) {
        input.val(log);
      }
    });
  }

  function avatarFileInputHandler(){
    // We can attach the `fileselect` event to all file inputs on the page
    $('.avatar-input').on('change', ':file', function() {
      var input = $(this),
        numFiles = input.get(0).files ? input.get(0).files.length : 1,
        label = input.val().replace(/\\/g, '/').replace(/.*\//, '');
      input.trigger('fileselect', [numFiles, label]);
    });

    $('.avatar-input-upload-input').on('fileselect', function(event, numFiles, label) {
      if($(this)[0].files.length > 0){
        var data = new FormData();
        data.append('image', $(this)[0].files[0]);
        var targeturl = $(this).attr('rel');

        $.ajax({
          type:'POST',
          url: targeturl,
          data: data,
          cache: false,
          contentType: false,
          processData: false,
          success:function(response){
            if (response.content) {
              $('.avatar-input-img').html(response.content);
            }
          },
          error: function(e){
            //console.log("error");
            //console.log(e);
          }
        });
      }
    });
  }

  customFileInputHandler();
  avatarFileInputHandler();

  function updatePackagePrice(promocode, package_id){

    $.ajax({
      type: 'post',
      url: '/orders/ajax-get-discount-package-price',
      data: {
        "package_id": package_id,
        "promocode": promocode
      },
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        $('.order-form .btn-submit span').text(response.price);
      }
      /*error: function(e) {
        console.log('error');
      }*/
    });
  }

  function updateCoursePrice(promocode, course_id, course_package_id){

    $.ajax({
      type: 'post',
      url: '/orders/ajax-get-discount-course-price',
      data: {
        "course_id": course_id,
        "promocode": promocode,
        "course_package_id": course_package_id
      },
      beforeSend: function (xhr) {
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      },
      success: function (response) {
        $('.order-form .btn-submit span').text(response.price);
      }
      /*error: function(e) {
        console.log('error');
      }*/
    });
  }

  $('#order-form-promocode-btn').on('click', function(){
    var package_id = $('.order-form').find('input[name=package_id]').val();
    var course_id = $('.order-form').find('input[name=course_id]').val();
    var course_package_id = $('.order-form').find('input[name=course_package_id]').val();
    var promocode = $('.order-form').find('input[name=promocode]').val().trim();

    if(typeof(promocode) != "undefined" && promocode !== null && promocode.length) {

      if(typeof(package_id) != "undefined" && package_id !== null && package_id.length){
        updatePackagePrice(promocode, package_id);
      }

      if(typeof(course_id) != "undefined" && course_id !== null && course_id.length &&
        typeof(course_package_id) != "undefined" && course_package_id !== null && course_package_id.length){
        updateCoursePrice(promocode, course_id, course_package_id);
      }
      else if(typeof(course_id) != "undefined" && course_id !== null && course_id.length){
        updateCoursePrice(promocode, course_id, null);
      }
    }
  });
});


$(document).ready(function(){

  $('.login-form').each(function(){
    $(this).validate({
      rules: {
        email: {
          required: true,
          email: true
        },
        password: {
          required: true,
          minlength: 6,
          maxlength: 255
        }
      },
      messages: {
        email: {
          required: i18next.t("Поле обязательно для заполнения"),
          email: i18next.t("Введите корректный email")
        },
        password: {
          required: i18next.t("Поле обязательно для заполнения"),
          minlength: i18next.t("Пароль должен содержать хотя бы 6 символов"),
          maxlength: i18next.t("Максимальная длина пароля 255 символов")
        }
      }
    });
  });

  $('.guest-login-form').each(function(){
    $(this).validate({
      rules: {
        email: {
          required: true,
          email: true
        },
        password: {
          required: true,
          minlength: 3,
          maxlength: 255
        }
      },
      messages: {
        email: {
          required: i18next.t("Поле обязательно для заполнения"),
          email: i18next.t("Введите корректный email")
        },
